{
  "name": "url-shrt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "db:dev": "dotenv -e .env.development -- npx prisma migrate dev",
    "db:docker": "dotenv -e .env.docker -- npx prisma migrate dev",
    "lint": "next lint",
    "start": "next start",
    "test": "jest --env=jsdom",
    "prepare": "husky install"
  },
  "dependencies": {
    "@prisma/client": "^2.28.0",
    "@styled-icons/material": "^10.34.0",
    "copy-to-clipboard": "^3.3.1",
    "date-fns": "^2.23.0",
    "is-public-domain": "^1.0.0",
    "is-valid-domain": "^0.1.2",
    "nanoid": "^3.1.23",
    "next": "11.0.1",
    "nextjs-redirect": "^5.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.3.0",
    "superjson": "^1.7.4"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/node": "^16.4.1",
    "@types/react": "^17.0.14",
    "@types/valid-url": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "babel-plugin-superjson-next": "^0.3.0",
    "dotenv": "^10.0.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.31.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "husky": ">=6",
    "jest": "^27.0.6",
    "lint-staged": ">=10",
    "node-mocks-http": "^1.10.1",
    "pg": "^8.6.0",
    "pgtools": "^0.3.2",
    "prettier": "^2.3.2",
    "prisma": "^2.28.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
    }
  }
}
